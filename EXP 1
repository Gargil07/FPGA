#Addition: 
ORG 00H 
MOV A, #30H 
MOV B, #20H 
ADD A, B 
MOV R0, A 
END 

#Subtraction 
ORG 00H 
MOV A, #30H 
MOV B, #20H 
SUBB A, B 
MOV R0, A 
END 

#Multiplication 
ORG 00H 
MOV A, #10H 
MOV B, #02H 
MUL AB 
MOV R0, A 
MOV R1, B 
END 

#Division 
ORG 00H 
MOV A, #06H 
MOV B, #04H 
DIV AB 
MOV R0, A 
MOV R1, B 
END
